<h1>Login</h1>

<%= form_tag '/login' do %>

  Email: <%= email_field_tag :email %>
  Password: <%= password_field_tag :password %>
  <%= submit_tag "Log Me In!" %>

<% end %>


<% if flash[:notice] %>
    <div class = "messages" >
        <p><%= link_to "Sign Up", signup_path %></p>
    </div>
  <% end %>